{
  "routes": [
    {
      "route": "/_nuxt/*",
      "headers": {
        "cache-control": "public,max-age=31536000,immutable"
      }
    },
    {
      "route": "/*",
      "statusCode": 301,
      "redirect": "https://www.thecodeveloper.com"
    },
    {
      "route": "/*",
      "rewrite": "/index.html"
    }
  ]
}
